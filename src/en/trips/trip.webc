---js 
{
  layout: "default",
  lang: "en",
  hasMap: true,
  pagination: { 
    data: "trips", 
    size: 1,
    alias: "trip",
    resolve: "values" 
  }, 
  breadcrumbs: [
    { name: "Home", link: "/" },
    { name: "Trips", link: "/trips/" },
  ],
  permalink: ({ trip }) =>`/en/trips/${trip.slug}/`,
  eleventyComputed: {
    title: ({ trip }) =>  trip.translate.en.title,
    description: ({ trip }) => trip.translate.en.description,
  },
} 
---

<trip :trip="trip" :lang="en"></trip>
