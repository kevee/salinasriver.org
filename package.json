    "clean-css": "^5.3.3",
    "dotenv": "^16.5.0",
    "exifr": "^7.1.3",
    "gray-matter": "^4.0.3",
    "heic-convert": "^2.1.0",
    "html-minifier": "^4.0.0",
    "i18next": "^24.2.3",
    "leaflet": "^2.0.0-alpha",
    "marked": "^15.0.12",
    "pagefind": "^1.3.0",
    "posthtml": "^0.16.6",
    "posthtml-minify-classnames": "^1.0.0-alpha.0",
    "puppeteer": "^24.10.0",
    "rollup": "^4.41.1",
